{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CosmosDb": {
    "Account": "#{CosmosAccount_From_KeyVault}",
    "Key": "#{CosmosKey_From_KeyVault}",
    "DatabaseName": "#{CosmosDatabase_From_KeyVault}",
    "ContainerName": "#{CosmosContainer_From_KeyVault}"
  },
  "BlobStorage": {
    "ConnectionString": "#{BlobConnectionString_From_KeyVault}",
    "ContainerName": "#{BlobContainer_From_KeyVault}"
  }
}
