{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CosmosDb": {
    "Account": "#{CosmosAccountName_From_KeyVault}",
    "Key": "#{CosmosKey_From_KeyVault}",
    "DatabaseName": "#{CosmosDatabaseName_From_KeyVault}",
    "ContainerName": "#{CosmosContainerName_From_KeyVault}"
  },
  "ImageStorage": {
    "ConnectionString": "#{BlobConnectionString_From_KeyVault}",
    "ContainerName": "#{BlobContainerName_From_KeyVault}"
  },
  "LogStorage": {
    "Account": "#{LogsAccountName_From_KeyVault}",
    "Key": "#{LogsKey_From_KeyVault}",
    "TableName": "#{LogTableName_From_KeyVault}"
  }
}
